    <style name="Theme.JWorrell_CIS651_Project_2.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.JWorrell_CIS651_Project_2.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
