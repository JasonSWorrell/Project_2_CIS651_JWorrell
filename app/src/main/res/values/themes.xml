<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.JWorrell_CIS651_Project_2" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#A9A9A9</item>
        <item name="colorOnPrimary">#A9A9A9</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#A9A9A9</item>
        <item name="colorSecondaryVariant">#A9A9A9</item>
        <item name="colorOnSecondary">@color/black</item>
